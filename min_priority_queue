#include<iostream>
using namespace std;
#include<queue>
// built in priority queue is max priority queue
// min priority queue
int main(){
    priority_queue < int,vector<int>,greater<int> >pq ;
    pq.push(16) ;
    pq.push(17) ;
    pq.push(18) ;
    pq.push(12) ;
    pq.push(19) ;
    cout << "size of priority queue :" << pq.size() << "\n" ;
    cout << "top- most element: " << pq.top() << "\n" ;
    while(!pq.empty()){
        cout << pq.top() << "\t" ;
        pq.pop() ;
    }

}
